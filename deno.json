{
  "$schema": "https://raw.githubusercontent.com/denoland/deno/refs/heads/main/cli/schemas/config-file.v1.json",
  "compilerOptions": {
    "types": ["npm:@telegraf/types"]
  },
  "tasks": {
    "dev": "deno run --watch --env-file --allow-all --unstable-cron --unstable-kv main.ts",
    "migrate": "deno run --unstable-kv migrate.ts",
    "deploy": "",
    "tunnel": "cloudflared tunnel run --token $(op read op://Project/flowerss-worker/tunnel-token)"
  },
  "imports": {
    "es-toolkit": "jsr:@es-toolkit/es-toolkit@^1.41.0",
    "@telegraf/types": "npm:@telegraf/types",
    "@std/assert": "jsr:@std/assert@1",
    "effect": "npm:effect",
    "fast-xml-parser": "npm:fast-xml-parser"
  }
}
